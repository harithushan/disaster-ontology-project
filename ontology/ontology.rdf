<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://www.example.org/disaster.owl"
         xmlns="http://www.example.org/disaster.owl#">

<owl:Ontology rdf:about="http://www.example.org/disaster.owl"/>

<owl:ObjectProperty rdf:about="#occursIn">
  <rdfs:domain rdf:resource="#Disaster"/>
  <rdfs:range rdf:resource="#Location"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasShelter">
  <rdfs:domain rdf:resource="#Location"/>
  <rdfs:range rdf:resource="#Shelter"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#requiresResource">
  <rdfs:domain rdf:resource="#Disaster"/>
  <rdfs:range rdf:resource="#Resource"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#assignedTo">
  <rdfs:domain rdf:resource="#Volunteer"/>
  <rdfs:range rdf:resource="#Shelter"/>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#hasName">
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#capacity">
  <rdfs:domain rdf:resource="#Shelter"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#severityLevel">
  <rdfs:domain rdf:resource="#Disaster"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#Disaster"/>

<owl:Class rdf:about="#Location"/>

<owl:Class rdf:about="#Shelter"/>

<owl:Class rdf:about="#Resource"/>

<owl:Class rdf:about="#Volunteer"/>

<owl:Class rdf:about="#Flood">
  <rdfs:subClassOf rdf:resource="#Disaster"/>
</owl:Class>

<owl:Class rdf:about="#Landslide">
  <rdfs:subClassOf rdf:resource="#Disaster"/>
</owl:Class>

<owl:Class rdf:about="#District">
  <rdfs:subClassOf rdf:resource="#Location"/>
</owl:Class>

<owl:NamedIndividual rdf:about="#Kandy">
  <rdf:type rdf:resource="#District"/>
  <hasShelter rdf:resource="#KandyTownHall"/>
  <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Kandy</hasName>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#KandyTownHall">
  <rdf:type rdf:resource="#Shelter"/>
  <capacity rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">300</capacity>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Ratnapura">
  <rdf:type rdf:resource="#District"/>
  <hasShelter rdf:resource="#RatnapuraSchool"/>
  <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ratnapura</hasName>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#RatnapuraSchool">
  <rdf:type rdf:resource="#Shelter"/>
  <capacity rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">500</capacity>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#FoodSupply">
  <rdf:type rdf:resource="#Resource"/>
  <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Food Supply</hasName>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#MedicalKit">
  <rdf:type rdf:resource="#Resource"/>
  <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Medical Kit</hasName>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Flood2024">
  <rdf:type rdf:resource="#Flood"/>
  <occursIn rdf:resource="#Ratnapura"/>
  <requiresResource rdf:resource="#FoodSupply"/>
  <requiresResource rdf:resource="#MedicalKit"/>
  <severityLevel rdf:datatype="http://www.w3.org/2001/XMLSchema#string">High</severityLevel>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Landslide2024">
  <rdf:type rdf:resource="#Landslide"/>
  <occursIn rdf:resource="#Kandy"/>
  <requiresResource rdf:resource="#MedicalKit"/>
  <severityLevel rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Medium</severityLevel>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Volunteer_01">
  <rdf:type rdf:resource="#Volunteer"/>
  <assignedTo rdf:resource="#KandyTownHall"/>
  <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Nimal Perera</hasName>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Flood2026">
  <rdf:type rdf:resource="#Flood"/>
  <occursIn rdf:resource="#Kandy"/>
  <requiresResource rdf:resource="#FoodSupply"/>
  <severityLevel rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Low</severityLevel>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#food 2026">
  <rdf:type rdf:resource="#Flood"/>
  <occursIn rdf:resource="#Kandy"/>
  <severityLevel rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Low</severityLevel>
</owl:NamedIndividual>


</rdf:RDF>
